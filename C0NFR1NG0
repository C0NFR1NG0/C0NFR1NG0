 <script>
 // Loading
 $(window).ready(function() {
 	setTimeout(function() {
 		$("#loading").fadeOut(100);
 		$("#site").removeClass('blur');
 	}, 900);
 });


 function triggerMenu() {
 	$("#nav-slide").toggle();
 	$("#mt").toggleClass('open');
 	$("#container").toggleClass('open');
 }
 $("#mt").click(function() {
 	triggerMenu();
 });
 </script>
 <script>
 // Autosubmit player form
 $("form#msodplayer select").change(function() {
 	var newPlayer = $(this).val();
 	dataLayer.push({
 		'event': 'GAEvent',
 		'eventCategory': 'music player',
 		'eventAction': 'change player',
 		'eventLabel': newPlayer,
 		'eventValue': 0,
 		'nonInteraction': false
 	});
 	$("form#msodplayer").submit();
 });

 // Social links open in new tab
 $(document).ready(function() {
 	$("#social-menu a").each(function() {
 		$(this).attr("target", "_blank");
 	});
 });


 // Run javascript on Gravity Form submit
 jQuery(document).ready(function(){

     jQuery(document).bind('gform_confirmation_loaded', function(event, formId){

 	    var formName = "form";

         if(formId == 1) {
             var formName = "new forum post";
         } else if(formId == 2) {
             var formName = "apply for authorization";
         } else if(formId == 3) {
             var formName = "new user registration";
         }

         // Push to dataLayer
         dataLayer.push({
         	'event': 'GAEvent',
         	'eventCategory': 'form',
         	'eventAction': 'submit',
         	'eventLabel': formName,
         	'eventValue': 0,
         	'nonInteraction': false
         });
     });

 });
 </script>


