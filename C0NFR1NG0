 <script>
 hashtag_regexp = /#([0-9]+)/g;
 siteurl = 'https://w41k3r.com';

 function linkHashtags(text) {
     return text.replace(
         hashtag_regexp,
         '<a class="walkerlink nolink" target="_blank" href="'+siteurl+'/walkers/?w_id=$1">#$1</a>'
     );
 }

 $(document).ready(function(){
     $('p,h1').each(function() {
         $(this).html(linkHashtags($(this).html()));
     });
 });
 </script>


